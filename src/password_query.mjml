<mjml>
<mj-head>
    <mj-title></mj-title>
    <mj-font name="Poppins" href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;600;700&display=swap" />
    <mj-attributes>
      <mj-all font-family="Poppins" />
      <mj-text font-size="16px" line-height="26px" />
      <mj-section background-color="#FFF" padding="20px 30px" />
      <mj-class name="aqua" color="#001934" />
      <mj-class name="blue" color="#1085E7" />
      <mj-class name="bold" font-weight="600" />

      <mj-class name="aquaLight" color="#012E48" font-weight="400" />
      <mj-class name="title" font-size="16px" font-weight="600" text="center" />

    
    </mj-attributes>
  </mj-head>

  <mj-body background-color="white" border-width="2px" border-color="#DCDCDC">
    <mj-container mj-class="body">
      <mj-section>
        <mj-column width="20%" vertical-align="middle">
          <mj-image
            width="0px"
            src="http://staging.aquaray.saas.hqhost.pl/img/logo_dark.jpg"
          />
        </mj-column>
        <mj-column width="80%" vertical-align="middle">
          <mj-text mj-class="aqua title">
            IMPORTER I DYSTRYBUTOR PROFESJONALNYCH PRODUKTÓW AKWARYSTYCZNYCH
          </mj-text>
        </mj-column>

    
        <!-- END: Header -->
        <!-- Header content -->
        <!-- END: Header content -->
      </mj-section>


  <mj-section>
    <mj-column padding-top="38px" width="100%">
          <mj-text mj-class="aquaLight">
            Dzień Dobry {company}<br />
            <mj-column mj-class="background-hero">
              <mj-text mj-class="title-hero"
                >${{ lang.password_reset_request_for_shop_name }}$</mj-text
              >
              <mj-divider mj-class="title-divider-hero"></mj-divider>
              <mj-text
                >${{
                lang.you_have_requested_to_reset_your_shop_name_login_details
                }}$</mj-text
              >
              <mj-spacer height="20px" />
              <mj-text
                >${{
                lang.please_note_that_this_will_change_your_current_password
                }}$</mj-text
              >
              <mj-spacer height="20px" />
              <mj-text
                >${{ lang.to_confirm_this_action_please_use_the_following_link
                }}$</mj-text
              >
              <mj-text padding-bottom="15px"><a href="{url}">{url}</a></mj-text>
            </mj-column>
          </mj-text>
        </mj-column>
        <mj-column width="100%" padding-bottom="10px"> </mj-column>
  </mj-section>

<mj-section>

      <mj-column>
        <mj-text  mj-class="aquaLight">
          AQUARAY Hurtownia Akwarystyczna ul. Spółdzielcza 1, 78-300 Świdwin
          NIP: PL 6721992035
        </mj-text>
      </mj-column>
      <mj-column>
        <mj-text padding-bottom="0px" mj-class="blue bold">
          Nr telefonu: 
                    <a href="mailto: hurt@aquaray.pl" style="font-weight:400;  color:#012E48; text-decoration: none">  515164361</a>

        
        </mj-text>

        <mj-text padding-bottom="0px"  mj-class="blue bold">
          E-mail: 
          <a href="mailto: hurt@aquaray.pl" style=" font-weight:400; color:#012E48; text-decoration: none">hurt@aquaray.pl</a>
        </mj-text />
        <mj-text  padding-bottom="0px" mj-class="blue bold">
          Strona: 
          <a href="http://www.aquaray.pl/" style="font-weight:400; color:#012E48;text-decoration: none">www.aquaray.pl</a>
        </mj-text />


      </mj-column>
    </mj-section>
    </mj-container>
  </mj-body>
</mjml>
